{
    "window.commandCenter": true,
    "workbench.colorTheme": "Solarized Light",
    "diffEditor.ignoreTrimWhitespace": true,
    "editor.formatOnSave": true,
    "notebook.formatOnSave.enabled": true,
    "files.insertFinalNewline": true,
    "files.autoSaveWhenNoErrors": true,
    "rubyLsp.rubyVersionManager": {
        "identifier": "asdf",
        "path": "/opt/homebrew/opt/asdf/libexec/asdf.sh"
    },
    "ruby.useLanguageServer": true,
    "ruby.format": "Shopify.ruby-lsp",
    "ruby.lint": {
        "rubocop": true
    },
    "ruby.rubocop.executePath": "/Users/jd/.asdf/shims/",
    "ruby.rubocop.onSave": true,
    "ruby.interpreter.commandPath": "/Users/jd/.asdf/shims/ruby",
    "ruby.useWorkspaceLibrary": true,
    "editor.lineNumbers": "on",
    "editor.wordWrap": "on",
    "workbench.editor.showLineNumbers": true,
    "editor.tabSize": 2,
    "editor.insertSpaces": true,
    "editor.detectIndentation": true,
    "redhat.telemetry.enabled": false,
    "dev.containers.defaultFeatures": {},
    "files.autoSave": "afterDelay",
    "rubyLsp.addonSettings": {},
    "rubyLsp.trace.server": "verbose",
    "ruby.debug.console": true,
    "ruby.trace.server": "verbose",
    "dotfiles.repository": "git@github.com:joofsh/coding_setup.git",
    "dotfiles.targetPath": "~/coding_setup",
    "dotfiles.installCommand": "~/coding_setup/install.sh",
    "remote.containers.dotfiles.repository": "git@github.com:joofsh/coding_setup.git",
    "remote.containers.dotfiles.targetPath": "~/coding_setup",
    "remote.containers.dotfiles.installCommand": "~/coding_setup/install.sh",
    "vscode-neovim.neovimExecutablePaths.darwin": "/usr/local/bin/nvim",
    "vscode-neovim.neovimInitVimPaths.darwin": "~/.config/nvim/init_cursor.lua",
    "vscode-neovim.neovimClean": true,
    "keyboard.dispatch": "keyCode",
    "git.decorations.enabled": true,
    "scm.diffDecorations": true,
    "vscode-neovim.useCtrlKeysForNormalMode": true,
    "vim.enableNeovim": true,
    "vim.neovimPath": "/usr/local/bin/nvim",
    "vim.neovimUseConfigFile": true,
    "vim.neovimConfigPath": "~/.config/nvim/init_cursor.lua",
    "search.exclude": {
        "**/tmp": true,
        "**/log": true,
        "**/storage": true,
        "**/node_modules": true,
        "**/vendor": true
    },
    "terminal.integrated.fontFamily": "Menlo, Monaco, 'Courier New', monospace",
    "terminal.integrated.fontSize": 13,
    "terminal.integrated.lineHeight": 1.2,
    "terminal.integrated.letterSpacing": 0.5,
    "terminal.integrated.gpuAcceleration": "auto",
    "terminal.integrated.hardware": true,
    "terminal.integrated.rendererType": "canvas",
    "terminal.integrated.fontLigatures": true,
    "terminal.integrated.minimumContrastRatio": 7,
    "terminal.integrated.fontWeight": 500,
    "[jsonc]": {
        "editor.defaultFormatter": "vscode.json-language-features"
    },
    "terminal.integrated.env.osx": {
        "PATH": ""
    },
    "terminal.integrated.inheritEnv": true,
    "terminal.integrated.defaultProfile.osx": "zsh",
    "terminal.integrated.profiles.osx": {
        "tmux-shell": {
            "path": "tmux",
            "args": [
                "new-session",
                "-A",
                "-s",
                "vscode:${workspaceFolder}"
            ]
        },
        "zsh": {
            "path": "/bin/zsh",
            "args": [
                "-l"
            ]
        }
    },
    "editor.accessibilitySupport": "on",
    "accessibility.signals.lineHasBreakpoint": {
        "sound": "off",
        "announcement": "auto"
    },
    "accessibility.signals.chatEditModifiedFile": {
        "sound": "off"
    },
